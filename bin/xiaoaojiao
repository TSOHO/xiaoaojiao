#!/usr/bin/env node

const program = require('commander')

program
  .version(require('../package').version)
  .usage('<command>')
  .command('list', '列出所有可用的模板')
  .command('create', '从模板生成一个新项目')
  .action(cmd => {
    if (cmd !== 'create' && cmd !== 'list') {
      program.outputHelp()
    }
  })

program.parse(process.argv)